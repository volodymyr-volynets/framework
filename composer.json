{
	"name": "numbers/framework",
	"type": "library",
	"description": "Lightweight MVC PHP Framework",
	"authors": [
		{
			"name": "Volodymyr Volynets",
			"email": "volodymyr.volynets@gmail.com"
		}
	],
	"require": {
		"php": ">=8.0.0",
		"oomphinc/composer-installers-extender": "~2.0"
	},
	"license": "Apache-2.0",
	"extra": {
		"installer-name": "Numbers/Framework",
		"installer-types": ["library"],
		"installer-paths": {
			"vendor/{$name}/": ["numbers/framework"]
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"oomphinc/composer-installers-extender": true
		}
	}
}